{
  "name": "Reall",
  "version": "0.1.0",
  "description": "Real Esate Manager & Services",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/absolute-fx/Reall.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --config webpack.common.js --watch",
    "start": "electron ."
  },
  "build": {
    "appId": "com.my.app",
    "publish": [
      {
        "provider": "github",
        "owner": "absolute-fx",
        "repo": "Reall"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "installerHeaderIcon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "dmg": {
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 345,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 125,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "keywords": [],
  "author": "AfxLab",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "edit-json-file": "^1.5.0",
    "electron": "^10.1.4",
    "electron-promise-ipc": "^2.2.1",
    "electron-updater": "^4.3.5",
    "google-map-react": "^2.1.9",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.0",
    "react-multi-lang": "^2.1.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-reload": "^1.5.0",
    "file-loader": "^6.1.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "ttf-loader": "^1.0.2",
    "ttf2woff": "^2.0.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  }
}
